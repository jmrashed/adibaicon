{
  "version": 2,
  "name": "adibaicon",
  "buildCommand": "npm run build:all",
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/dist/(.*)",
      "dest": "/dist/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/font/(.*)",
      "dest": "/font/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/dist/css/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/dist/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}