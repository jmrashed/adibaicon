{
  "name": "adibaicon",
  "version": "2.2.0",
  "description": "A lightweight, customizable icon font library with 78+ letter-based and UI icons in multiple styles. Includes React/Vue components, accessibility features, and PWA support.",
  "main": "dist/css/adibaIcon.css",
  "module": "dist/js/adibaIcon.esm.js",
  "browser": "dist/css/adibaIcon.min.css",
  "style": "dist/css/adibaIcon.css",
  "sass": "src/scss/adibaIcon.scss",
  "files": [
    "dist/",
    "src/scss/",
    "src/less/",
    "integrations/",
    "README.md",
    "LICENSE.txt",
    "DISTRIBUTION.md"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "build:webpack": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "optimize:fonts": "node scripts/optimize-fonts.js",
    "build:all": "npm run build && npm run optimize:fonts",
    "postinstall": "node scripts/postinstall.js",
    "prepublishOnly": "npm run build:all",
    "test": "echo \"Tests will be added in future versions\" && exit 0",
    "clean": "rmdir /s /q dist 2>nul || echo \"Dist cleaned\"",
    "generate:svg": "node scripts/generate-svg.js",
    "generate:docs": "echo \"Icon documentation generated\"",
    "cdn:build": "node scripts/cdn-build.js",
    "release": "node scripts/release.js",
    "git:release": "node scripts/git-release.js",
    "serve": "echo \"Serve with development server\"",
    "deploy": "node scripts/quick-deploy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmrashed/adibaicon.git"
  },
  "keywords": [
    "icon",
    "font",
    "css",
    "icons",
    "web-font",
    "typography",
    "ui",
    "design",
    "letters",
    "alphabet",
    "adibaicon",
    "react",
    "vue",
    "angular",
    "accessibility",
    "pwa",
    "responsive",
    "lightweight",
    "customizable",
    "open-source",
    "frontend",
    "web-components",
    "scss",
    "sass",
    "less"
  ],
  "author": {
    "name": "Md Rasheduzzaman",
    "email": "jmrashed@gmail.com",
    "url": "https://github.com/jmrashed"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmrashed/adibaicon/issues"
  },
  "homepage": "https://adibaicon.vercel.app",
  "cdn": "https://cdn.jsdelivr.net/npm/adibaicon@latest/dist/css/adibaIcon.min.css",
  "jsdelivr": "dist/css/adibaIcon.min.css",
  "unpkg": "dist/css/adibaIcon.min.css",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/jmrashed"
  },
  "engines": {
    "node": "18.x"
  },
  "devDependencies": {},
  "dependencies": {}
}